  <table class="table table-hover table-inverse">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  
  
  
  <tbody>
	  <tr> 
	 <% users.each do |user| %>
      <td>
        <%= link_to user_path(user) do %>
        <%= attachment_image_tag user, :profile_image, fallback: "no_image.jpg", size: "40x40"%>
        <%= user.name %><br>
        <% end %>
      </td>
      <td> <%= user.name %></td>
      
      <td><%= render 'relationships/info', user: user %></td>
	  	<td><%= link_to "Show", "/users/#{user.id}" %></td
    <% end %>
    </tr>
</table>